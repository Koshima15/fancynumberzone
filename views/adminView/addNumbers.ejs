<%- include('../partials/adminTop') %> <%- include('../partials/adminTopBar') %>
<!-- #Top Bar -->
<%- include('../partials/adminSideBar') %>

<section class="content">
  <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
          <div class="card">
            <div class="header">
              <h2>Add Numbers</h2>
            </div>
            <div class="body">
              <div class="row clearfix">
                <div class="col-sm-3">
                  <h2 class="card-inside-title">Mobile No.</h2>
                  <input type="text" class="form-control" placeholder="Enter Mobile No." />
                </div>
                <div class="col-sm-3">
                  <h2 class="card-inside-title">Category</h2>
                  <select class="form-control">
                    <option value="">-- Select Category--</option>
                    <option value="10">Category A</option>
                    <option value="20">Category B</option>
                    <option value="30">Category C</option>
                    <option value="40">Category D</option>
                    <option value="50">Category E</option>
                </select>
                </div>
                <div class="col-sm-3">
                  <h2 class="card-inside-title">Vendor</h2>
                  <select class="form-control">
                    <option value="">-- Select Vendor--</option>
                    <option value="10">Vendor A</option>
                    <option value="20">Vendor B</option>
                    <option value="30">Vendor C</option>
                    <option value="40">Vendor D</option>
                    <option value="50">Vendor E</option>
                </select>
                </div>
                <div class="col-sm-2">
                  <h2 class="card-inside-title">Price</h2>
                  <input type="text" class="form-control" placeholder="Enter Price"/>
                </div>
                <div class="col-sm-1">
                  <br>
                  <button type="button" class="btn btn-success btn-circle waves-effect waves-circle waves-float">
                    <i class="material-icons">add</i>
                </button>
                </div>
              </div>
              <button type="button" class="btn btn-primary m-t-15 waves-effect">Submit</button>
              <button type="button" class="btn btn-danger m-t-15 waves-effect">Cancel</button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<%- include('../partials/adminBottom') %>
